{
  "name": "@craftgear/extract-prompts",
  "version": "3.0.0",
  "description": "Extract ComfyUI workflow JSON and A1111 prompts from images and videos",
  "license": "MIT",
  "author": "",
  "keywords": [
    "comfyui",
    "workflow",
    "prompt",
    "extraction",
    "image",
    "video",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craftgear/extract-prompts.git"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": {
    "extract-prompts": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "oxlint src",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@vitest/ui": "^3.2.4",
    "oxlint": "^1.6.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "dependencies": {
    "commander": "^14.0.0",
    "exifr": "^7.1.3",
    "ffprobe-static": "^3.1.0",
    "glob": "^11.0.3",
    "sharp": "^0.34.2"
  }
}
